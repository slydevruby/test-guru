<tr>
    <td class="align-middle">
        <%= link_to test.title, admin_test_path(test), data: { test_id: test.id }, class: 'test-title' %>
        <%= render 'form_inline', test: test == @test ? @test : test %>
    </td>
    <td class="align-middle">
        <%= test.category.title %>
    </td>
    <td class="align-middle">
        <%= test_level(test.level) %>
    </td>
    <td class="align-middle">
        <%= test.questions.count %>
    </td>
    <td class="align-middle">
        <%= link_to t('.edit_inline'),
      edit_admin_test_path(test),
      data: { test_id: test.id }, class: 'form-inline-link'
     %> |
        <%= link_to t('.edit'), edit_admin_test_path(test) %>
    </td>
</tr>