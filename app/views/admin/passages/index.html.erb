<table class='table'>
  <thead>
    <tr>
      <th>User</th>
      <th>Test</th>
      <th>Grade</th>
      <th>Date</th>
      <th>Delete</th>
    </tr>
  </thead>
  <tbody>
    <% @passages.each do |passage| %>
      <tr>
        <td><%= passage.user.email %></td>
        <td><%= passage.test.title %></td>
        <td><%= passage.grade %></td>
        <td><%= passage.created_at %></td>
        <td><%= button_to 'Delete', admin_passage_path(passage), method: :delete,
        class: 'btn btn-danger' %></td>
      </tr>
    <% end %>
  </tbody>
</table>
