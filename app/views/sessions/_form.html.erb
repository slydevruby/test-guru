<%= form_with url: sessions_path, data: { turbo: false } do |form| %>
  <%= flash_messages %>
  <p>
    <%= form.label :email %><br>
    <%= form.text_field :email %>
  </p>
  <p>
    <%= form.label :password %><br>
    <%= form.text_field :password %>
  </p>
  <p><%= form.submit 'Enter' %></p>
<% end %>
