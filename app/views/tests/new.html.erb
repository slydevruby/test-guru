<%= form_with model: @test do |form| %>
  <div>
    <%= form.label :title %><br>
    <%= form.text_field :title %>

    <% @test.errors.full_messages.each do |message| %>
      <div><%= message %></div>
    <% end %>    
  </div>

  <div>
    <%= form.label :level %><br>
    <%= form.number_field :level %>

  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
